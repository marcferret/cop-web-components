{
  "name": "mng-starter",
  "version": "1.0.0",
  "description": "MANGO starter",
  "main": "index.jsx",
  "repository": {
    "type": "git",
    "url": "https://jira.mangodev.net/stash/scm/fron/mng-starter"
  },
  "publishConfig": {
    "registry": "https://citools.mangodev.net/nexus/content/repositories/npmjsmango/"
  },
  "component": "helloMNG",
  "scripts": {
    "start:shop:mobile": "cross-var webpack-dev-server --open --env.context=shop --env.device=mobile --env.component=$npm_package_component --env.NODE_ENV=local",
    "start:shop:tablet": "cross-var webpack-dev-server --open --env.context=shop --env.device=tablet --env.component=$npm_package_component --env.NODE_ENV=local",
    "start:shop:desktop": "cross-var webpack-dev-server --open --env.context=shop --env.device=desktop --env.component=$npm_package_component --env.NODE_ENV=local",
    "dev:shop": "cross-var npm-run-all dev:shop:*",
    "dev:shop:mobile": "cross-var webpack -d --env.context=shop --env.device=mobile --env.component=$npm_package_component --env.NODE_ENV=dev --watch",
    "dev:shop:tablet": "cross-var webpack -d --env.context=shop --env.device=tablet --env.component=$npm_package_component --env.NODE_ENV=dev --watch",
    "dev:shop:desktop": "cross-var webpack -d --env.context=shop --env.device=desktop --env.component=$npm_package_component --env.NODE_ENV=dev --watch",
    "pro:shop": "cross-var npm-run-all pro:shop:*",
    "pro:shop:mobile": "cross-var webpack -p --env.context=shop --env.device=mobile --env.component=$npm_package_component --env.NODE_ENV=pro",
    "pro:shop:tablet": "cross-var webpack -p --env.context=shop --env.device=tablet --env.component=$npm_package_component --env.NODE_ENV=pro",
    "pro:shop:desktop": "cross-var webpack -p --env.context=shop --env.device=desktop --env.component=$npm_package_component --env.NODE_ENV=pro",
    "start:outlet:mobile": "cross-var webpack-dev-server --open --env.context=outlet --env.device=mobile --env.component=$npm_package_component --env.NODE_ENV=local",
    "start:outlet:tablet": "cross-var webpack-dev-server --open --env.context=outlet --env.device=tablet --env.component=$npm_package_component --env.NODE_ENV=local",
    "start:outlet:desktop": "cross-var webpack-dev-server --open --env.context=outlet --env.device=desktop --env.component=$npm_package_component --env.NODE_ENV=local",
    "dev:outlet": "cross-var npm-run-all dev:outlet:*",
    "dev:outlet:mobile": "cross-var webpack -d --env.context=outlet --env.device=mobile --env.component=$npm_package_component --env.NODE_ENV=dev --watch",
    "dev:outlet:tablet": "cross-var webpack -d --env.context=outlet --env.device=tablet --env.component=$npm_package_component --env.NODE_ENV=dev --watch",
    "dev:outlet:desktop": "cross-var webpack -d --env.context=outlet --env.device=desktop --env.component=$npm_package_component --env.NODE_ENV=dev --watch",
    "pro:outlet": "cross-var npm-run-all pro:outlet:*",
    "pro:outlet:mobile": "cross-var webpack -p --env.context=outlet --env.device=mobile --env.component=$npm_package_component --env.NODE_ENV=pro",
    "pro:outlet:tablet": "cross-var webpack -p --env.context=outlet --env.device=tablet --env.component=$npm_package_component --env.NODE_ENV=pro",
    "pro:outlet:desktop": "cross-var webpack -p --env.context=outlet --env.device=desktop --env.component=$npm_package_component --env.NODE_ENV=pro",
    "pro": "cross-var npm-run-all -p pro:shop pro:outlet",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "bundle-analyzer": "cross-var webpack -p --env.context=shop --env.device=desktop --env.component=$npm_package_component --env.NODE_ENV=pro --env.analyzer=true"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ]
  },
  "author": "Equipo ecommerce frontend <eqp.ecommerce.frontend@mango.com>",
  "contributors": [
    "FrontEnd Team <eqp.ebusiness.frontend@mango.com>"
  ],
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "bourbon": "^4.3.4",
    "cross-var": "^1.1.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-to-json": "^3.1.2",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^3.0.1",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "mng-styleguide": "https://jira.mangodev.net/stash/scm/fron/mng-styleguide.git",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.7",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "sasslint-loader": "0.0.1",
    "sasslint-webpack-plugin": "^1.0.4",
    "susy": "^2.2.12",
    "webpack": "^3.7.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^2.9.1",
    "webpack-rtl-plugin": "^1.6.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "dotenv-webpack": "^1.5.5",
    "element-dataset": "^2.2.6",
    "es6-promise": "^4.1.1",
    "fbemitter": "^2.1.1",
    "lodash": "^4.17.4",
    "mdn-polyfills": "^5.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "universal-cookie": "^2.1.0"
  }
}
