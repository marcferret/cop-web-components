{
  "name": "mng-starter",
  "version": "1.0.0",
  "description": "MANGO starter",
  "main": "index.jsx",
  "repository": {
    "type": "git",
    "url": "https://jira.mangodev.net/stash/scm/fron/mng-starter"
  },
  "scripts": {
    "start:shop:mobile": "webpack-dev-server -d --env.context=shop --env.device=mobile",
    "start:shop:tablet": "webpack-dev-server -d --env.context=shop --env.device=tablet",
    "start:shop:desktop": "webpack-dev-server -d --env.context=shop --env.device=desktop",
    "dev:shop": "npm-run-all dev:shop:*",
    "dev:shop:mobile": "webpack -d --env.context=shop --env.device=mobile -w",
    "dev:shop:tablet": "webpack -d --env.context=shop --env.device=tablet -w",
    "dev:shop:desktop": "webpack -d --env.context=shop --env.device=desktop -w",
    "pro:shop": "npm-run-all pro:shop:*",
    "pro:shop:mobile": "webpack -p --env.context=shop --env.device=mobile",
    "pro:shop:tablet": "webpack -p --env.context=shop --env.device=tablet",
    "pro:shop:desktop": "webpack -p --env.context=shop --env.device=desktop",
    "start:outlet:mobile": "webpack-dev-server --env.context=outlet --env.device=mobile",
    "start:outlet:tablet": "webpack-dev-server --env.context=outlet --env.device=tablet",
    "start:outlet:desktop": "webpack-dev-server --env.context=outlet --env.device=desktop",
    "dev:outlet": "npm-run-all dev:outlet:*",
    "dev:outlet:mobile": "webpack -d --env.context=outlet --env.device=mobile -w",
    "dev:outlet:tablet": "webpack -d --env.context=outlet --env.device=tablet -w",
    "dev:outlet:desktop": "webpack -d --env.context=outlet --env.device=desktop -w",
    "pro:outlet": "npm-run-all pro:outlet:*",
    "pro:outlet:mobile": "webpack -p --env.context=outlet --env.device=mobile",
    "pro:outlet:tablet": "webpack -p --env.context=outlet --env.device=tablet",
    "pro:outlet:desktop": "webpack -p --env.context=outlet --env.device=desktop",
    "pro": "npm-run-all -p pro:shop pro:outlet",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "deploy": "node deploy.js",
    "copy": "node copy.js",
    "bundle-analyzer": "webpack -p --env.context=shop --env.device=desktop --env.analyzer=true"
  },
  "author": "Equipo ecommerce FrontEnd <eqp.ecommerce.frontend@mango.com>",
  "contributors": [
    "FrontEnd Team <eqp.ebusiness.frontend@mango.com>"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ]
  },
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "bourbon": "^4.3.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^3.0.1",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "mng-styleguide": "https://jira.mangodev.net/stash/scm/fron/mng-styleguide.git",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.0.7",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "sasslint-loader": "0.0.1",
    "sasslint-webpack-plugin": "^1.0.4",
    "susy": "^2.2.12",
    "webpack": "^3.7.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^2.9.1",
    "webpack-rtl-plugin": "^1.6.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "dotenv": "^5.0.1",
    "dotenv-webpack": "^1.5.5",
    "element-dataset": "^2.2.6",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-to-json": "^3.1.2",
    "es6-promise": "^4.1.1",
    "fbemitter": "^2.1.1",
    "lodash": "^4.17.4",
    "mdn-polyfills": "^5.4.0",
    "ncp": "^2.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "scp2": "^0.5.0",
    "universal-cookie": "^2.1.0"
  }
}
